class Person:
    def __init__(self, name="Unknown", age=0):
        self.name = name
        self.age = age
    
    def display(self):
        print(f"Name: {self.name}, Age: {self.age}")

# Different ways to create objects
p1 = Person()  # Default values
p2 = Person("Alice")  # Only name provided
p3 = Person("Bob", 25)  # Both name and age provided

p1.display()
p2.display()
p3.display()
 
